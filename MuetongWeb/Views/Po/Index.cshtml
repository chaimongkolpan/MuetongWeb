@using Newtonsoft.Json;
@{
    ViewData["Title"] = "สั่งซื้อ";
}
<div class="container-fluid" style="background-color:#fcfcfc">
    <div class="row row-cols-1 row-cols-md-2">
        <div class="col">
            <div class="input-group mb-3">
                <label class="input-group-text" for="ProjectName"><b>ชื่อโครงการ</b></label>
                <select class="form-select" id="ProjectId" name="ProjectId">
                    <option value="0" selected>ทั้งหมด</option>
                    @if (Model != null)
                    {
                        @foreach (var item in Model.Projects)
                        {
                            <option value="@item.Id">@item.Name</option>
                        }
                    }
                </select>
            </div>
        </div>
        <div class="col">
            <div class="input-group mb-3">
                <label class="input-group-text" for="PrNo"><b>เลขที่ PR</b></label>
                <select class="form-select" id="PrNo" name="PrNo">
                    <option selected>ทั้งหมด</option>
                </select>
            </div>
        </div>
        <div class="col">
            <div class="input-group mb-3">
                <label class="input-group-text" for="PoNo"><b>เลขที่ PO</b></label>
                <select class="form-select" id="PoNo">
                    <option selected>ทั้งหมด</option>
                </select>
            </div>
        </div>
        <div class="col">
            <div class="input-group mb-3">
                <label class="input-group-text" for="RequesterId"><b>ผู้สั่งสินค้า</b></label>
                <select class="form-select" id="RequesterId" name="RequesterId">
                    <option value="0" selected>ทั้งหมด</option>
                </select>
            </div>
        </div>
        <div class="col">
            <div class="input-group mb-3">
                <label class="input-group-text" for="Status"><b>สถานะสินค้า</b></label>
                <select class="form-select" id="Status" name="Status">
                    <option selected>ทั้งหมด</option>
                    <option>รอตรวจสอบสั่งสินค้า</option>
                    <option>สั่งสินค้า</option>
                    <option>รอสั่งซื้อ</option>
                    <option>รอสินค้าจัดส่ง</option>
                    <option>จัดส่งสำเร็จ</option>
                </select>
            </div>
        </div>
    </div>
    <div class="d-flex justify-content-center ">
        <button type="button" id="search_btn" class="btn btn-outline-success"><i class="fa fa-search"> &nbsp;ค้นหา</i></button>
    </div>
    <div class="d-flex justify-content-end ">
        <button type="button" id="add_btn" class="btn btn-warning" data-bs-toggle="modal" data-bs-target="#AddPurchase">
            <i class="fa fa-plus"> &nbsp;เพิ่มการสั่งซื้อ</i>
        </button>
    </div>


